##### a.asm
.text:
	global print_char

; rdi - char c
print_char:
	push rdi
	mov rsi, rsp ; char*
	mov rdx, 1 ; len
	mov rax, 1 ; sys_write
	mov rdi, 1 ; stdout
	syscall
	pop rdi
	ret
#####

##### b.asm
extern print_char

global _start

section .text
_start:
	mov rdi, 'b' ; newline
	call print_char
	mov rdi, 10 ; newline
	call print_char

    mov rax, 60 ; sys_exit
	mov rdi, 0 ; return code
	syscall
#####

Build and run:
nasm -f elf64 a.asm -o a.o
nasm -f elf64 b.asm -o b.o
ld -m elf_x86_64 a.o b.o a.out
./a.out